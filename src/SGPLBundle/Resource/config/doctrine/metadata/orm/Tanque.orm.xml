<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Tanque" table="tanque">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="identificador" type="string" column="identificador" length="15" nullable="false"/>
    <field name="nombre" type="string" column="nombre" length="45" nullable="false"/>
    <field name="numeroSerie" type="string" column="numero_serie" length="15" nullable="false"/>
    <field name="descripcion" type="text" column="descripcion" length="65535" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>
