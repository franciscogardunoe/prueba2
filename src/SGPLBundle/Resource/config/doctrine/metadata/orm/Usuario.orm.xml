<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Usuario" table="usuario">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="usuario" type="string" column="usuario" length="45" nullable="false"/>
    <field name="correoElectronico" type="string" column="correo_electronico" length="45" nullable="false"/>
    <field name="contrasexa" type="string" column="contrasexa" length="70" nullable="false"/>
    <field name="fechaRegistro" type="date" column="fecha_registro" nullable="false"/>
    <field name="status" type="integer" column="status" nullable="false"/>
  </entity>
</doctrine-mapping>
