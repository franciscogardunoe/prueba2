<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Propietario" table="propietario">
    <indexes>
      <index name="fk_propietario_persona1_idx" columns="persona"/>
      <index name="fk_propietario_centro_produccion1_idx" columns="centro_produccion"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="persona" target-entity="Persona">
      <join-columns>
        <join-column name="persona" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="centroProduccion" target-entity="CentroProduccion">
      <join-columns>
        <join-column name="centro_produccion" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
